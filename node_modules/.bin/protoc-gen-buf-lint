#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/@bufbuild+buf@1.57.2/node_modules/@bufbuild/buf/bin/node_modules:/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/@bufbuild+buf@1.57.2/node_modules/@bufbuild/buf/node_modules:/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/@bufbuild+buf@1.57.2/node_modules/@bufbuild/node_modules:/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/@bufbuild+buf@1.57.2/node_modules:/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/@bufbuild+buf@1.57.2/node_modules/@bufbuild/buf/bin/node_modules:/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/@bufbuild+buf@1.57.2/node_modules/@bufbuild/buf/node_modules:/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/@bufbuild+buf@1.57.2/node_modules/@bufbuild/node_modules:/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/@bufbuild+buf@1.57.2/node_modules:/Users/leoyang/Desktop/NEW_CHAPTER/backend/proto/scoreboard-streaming-lab/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@bufbuild/buf/bin/protoc-gen-buf-lint" "$@"
else
  exec node  "$basedir/../@bufbuild/buf/bin/protoc-gen-buf-lint" "$@"
fi
